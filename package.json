{
  "name": "klan",
  "version": "0.0.1",
  "description": "Koa, LokiJS, Angular 2 and Node application",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean-dev": "rimraf dev && rm -f npm-debug.log*",
    "build-client": "webpack --config src-a/client/config/webpack.prod.babel.js --progress",
    "build-server": "tsc --project src-a/server/ --outDir dist/",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "build-dev-client": "webpack --config src/client/config/webpack.dev.babel.js --progress",
    "build-dev-server": "tsc --project src/server/ --outDir dev/ --sourceMap",
    "build-dev": "npm run clean-dev && npm run build-dev-server & npm run build-dev-client",
    "dev": "npm run build-dev && nodemon --watch dev/server dev/server/app.js & npm run watch",
    "livereload": "$(npm bin)/browser-sync start --proxy 'http://localhost:3000' --files 'dev/client/**/*'",
    "start": "node dist/server/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run watch-dev-client & npm run watch-dev-server",
    "watch-dev-client": "webpack --config src-a/client/config/webpack.dev.babel.js --progress --watch",
    "watch-dev-server": "tsc --project src-a/server/ --outDir dev/ --sourceMap --watch"
  },
  "keywords": [
    "koa",
    "lokijs",
    "angular2",
    "node",
    "javascript"
  ],
  "author": "hacked",
  "license": "MIT",
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.5",
    "@angular/cli": "~8.3.5",
    "@angular/compiler-cli": "~8.2.7",
    "@angular/language-service": "~8.2.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jsonwebtoken": "7.1.33",
    "@types/koa": "2.0.39",
    "@types/koa-bodyparser": "3.0.23",
    "@types/koa-router": "7.0.22",
    "@types/koa-static": "2.0.22",
    "@types/lokijs": "1.2.30",
    "@types/node": "~8.9.4",
    "angular2-template-loader": "0.4.0",
    "awesome-typescript-loader": "2.2.4",
    "babel-cli": "6.18.0",
    "babel-preset-latest": "6.16.0",
    "browser-sync": "^2.26.7",
    "browsersync": "0.0.1-security",
    "codelyzer": "^5.0.1",
    "css-loader": "0.23.1",
    "css-to-string-loader": "0.1.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "2.15.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "4.0.0",
    "nodemon": "^1.19.3",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.7",
    "protractor": "~5.4.0",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "1.6.1",
    "rimraf": "2.5.2",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",

    "@angular/platform-browser-dynamic": "~8.2.7",
    "@angular/router": "~8.2.7",
    "@angular/http": "2.2.0",
    "angular2-jwt": "0.2.2",
    "bootstrap-sass": "3.3.7",
    "cerialize": "0.1.13",
